{
	// Place your snippets for javascript here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
  "React component return function with auto tag wrap": {
    "prefix": "reat",
    "body": [
      "return(\n\t<>\n\t\t$0\n\t</>\n)"
    ]
  },
  "Mui shortcut": {
    "prefix": "mui",
    "body": [
      "'@material-ui/core$0'"
    ]
  },
  "Named require snippet": {
      "prefix": "requi",
      "body": [
        "const ${1:moduleName} = require(\"${2:modulePath}\");$0"
      ]
  },
  "Express fast import": {
    "prefix": "express",
    "body": [
      "const express = require('express');\n$0"
    ]
  },
  "process.env snippet": {
    "prefix": "env",
    "body": [
      "process.env.$0"
    ]
  },
  "curl snippet": {
    "prefix": "curl",
    "body": [
      "{$0}"
    ]
  },
  "box bracket snippet": {
    "prefix": "box",
    "body": [
      "[$0]"
    ]
  },
  "quote snipper": {
    "prefix": "quote",
    "body": [
      "'$0'"
    ]
  },
  "double quote snipper": {
    "prefix": "qquote",
    "body": [
      "\"$0\""
    ]
  },
  "underscore snipper": {
    "prefix": "underscore",
    "body": [
      "_$0"
    ]
  },
  "getServerSideProps nextjs snippet": {
    "prefix": "ssprop",
    "body": [
      "export async function getServerSideProps(context) {\n\tconst { $1 } = context.$2;\n\n\treturn { props: { $1 }\n\t} \n}"
    ]
  },
  "getStaticProps nextjs snippet": {
    "prefix": "stprop",
    "body": [
      "export async function getStaticProps(context) {\n\tconst { $1 } = context.$2;\n\n\treturn { paths,\n\tfallback: false\n\t} \n}"
    ]
  },
  "arrow function":{
    "prefix": "arfn",
    "body": [
      "($1) => {\n\t$0\n}"
    ]
  },
  "if snippet":{
    "prefix": "if",
    "body": [
      "if ($1) {\n\tthrow $1\n}\n$0"
    ]
  },
  "import user written modules": {
    "prefix": "impo",
    "body": [
      "import ${1:moduleName} from '${2:path}'\n$0"
    ]
  },
  "named arrow function":{
    "prefix": "naf",
    "body": [
      "const ${1:name} = (${2:param}) => {\n\t$0\n}"
    ]
  },
  "export default": {
    "prefix": "expd",
    "body": [
      "export default ${0:moduleName};"
    ]
  }
}
